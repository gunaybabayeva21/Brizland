@using BrizLand.ViewModels;
@model Servis
<h1>Servis Edit</h1>
<form class="Col-8" method="post" enctype="multipart/form-data">

    <div class="form-group">
        <label asp-for="Title"></label>
        <input asp-for="Title" class="form-control" id="exampleInputPassword1" placeholder="Name">
        <span class="text-denger" asp-validation-for="Title"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description"></label>
        <input asp-for="Description" class="form-control" id="exampleInputPassword1" placeholder="Work">
        <span class="text-denger" asp-validation-for="Description"></span>
    </div>
    <div class="form-group">
        <label asp-for="IconName"></label>
        <input asp-for="IconName" class="form-control" id="exampleInputPassword1" placeholder="ImageUrl">
        <span class="text-denger" asp-validation-for="IconName"></span>

    </div>
    <button type="submit" class="btn btn-primary">Edit</button>
</form>
